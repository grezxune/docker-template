{
  "name": "docker-template-api",
  "version": "0.0.1",
  "description": "API for docker template",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:dev": "cross-env DEBUG=neo4j-graphql-js ./node_modules/.bin/nodemon --watch src --ext js,graphql --exec babel-node  src/index.js",
    "build": "babel src --out-dir build && shx cp .env build 2>/dev/null || : && shx cp src/schema.graphql build",
    "now-build": "babel src --out-dir build && shx cp src/schema.graphql build",
    "start": "yarn build && node build/index.js",
    "seedDb": "./node_modules/.bin/babel-node src/seed/seed-db.js"
  },
  "author": "Tommy Treb",
  "license": "MIT",
  "dependencies": {
    "apollo-server": "^2.15.1",
    "apollo-server-express": "^2.15.1",
    "bcrypt": "^5.0.0",
    "dotenv": "^7.0.0",
    "express": "^4.17.1",
    "graphql-auth-directives": "^2.2.0",
    "graphql-tag": "^2.10.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.27.0",
    "neo4j-driver": "^4.0.2",
    "parse-neo4j": "^0.6.11",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime-corejs3": "^7.9.2",
    "babel-plugin-auto-import": "^1.0.5",
    "babel-plugin-module-resolver": "^4.0.0",
    "cross-env": "^7.0.2",
    "nodemon": "^1.19.1",
    "shx": "^0.3.2"
  }
}
